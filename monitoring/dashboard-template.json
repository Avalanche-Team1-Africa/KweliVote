                },
                {
                  "name": "Query",
                  "value": "traces\n| where customDimensions.operation startswith \"blockchain_\"\n| summarize count() by operation=tostring(customDimensions.operation), status=tostring(customDimensions.status), bin(timestamp, 15m)\n| render timechart"
                },
                {
                  "name": "PartTitle",
                  "value": "Blockchain Transactions"
                },
